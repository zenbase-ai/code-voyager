{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "skills": {
    "paths": [
      ".claude/skills/local",
      ".claude/skills/generated"
    ]
  },
  "permissions": {
    "allow": [
      "Bash(python:*)",
      "Bash(ruff check:*)",
      "Bash(skill-index:*)",
      "Bash(find-skill:*)"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/session_start.py",
            "timeout": 10000
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/pre_compact.py",
            "timeout": 20000
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/session_end.py",
            "timeout": 20000
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "python3 '/Users/knrz/Git/zenbase-ai/voyager/.claude/hooks/post_tool_use_feedback.py'",
            "timeout": 5000
          }
        ]
      }
    ]
  }
}
